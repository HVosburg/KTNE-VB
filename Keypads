                ElseIf blnPic1Show = True Then

                    picAns2.Image = Image.FromFile("Z:\KTNE VB\Solutions\Keypads\Pics\22-rightc.png")

                    picAns2.Show()
                    blnPic2Show = True

                ElseIf blnPic1Show = False Then

