                    picAns4.Show()
                    blnPic4Show = True

                End If

            End If

            'Check if all match lst6------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
        ElseIf lst6.Contains(input1) And lst6.Contains(input2) And lst6.Contains(input3) And lst6.Contains(input4) Then

